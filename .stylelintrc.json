{
  "extends": [
    "stylelint-config-standard-scss",
    "stylelint-config-idiomatic-order"
  ],
  "plugins": [
    "stylelint-order",
    "stylelint-scss"
  ],
  "rules": {
    "no-empty-source": null
  },
  "ignoreFiles": [
    "dist/**",
    "src/assets/github-markdown-*.css",
    "src/assets/highlightjs-*.css"
  ]
}
